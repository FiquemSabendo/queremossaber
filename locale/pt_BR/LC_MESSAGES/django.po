#: web/foi_requests/models.py:84
msgid "Local Public Bodies must have a \"UF\"."
msgstr "Órgãos municipais devem ter um \"Estado\"."

#: web/foi_requests/models.py:86
msgid "Local Public Bodies must have a \"municipality\"."
msgstr "Órgãos municipais devem ter uma \"cidade\"."

#: web/foi_requests/models.py:89
msgid "State Public Bodies must have a \"UF\"."
msgstr "Órgãos estaduais devem ter um \"Estado\"."

#: web/foi_requests/models.py:91
msgid "State Public Bodies must not have a \"municipality\"."
msgstr "Órgãos estaduais não podem ter uma \"cidade\"."

#: web/foi_requests/models.py:95
msgid "Federal Public Bodies must not have a \"UF\"."
msgstr "Órgãos federais não podem ter um \"Estado\"."

#: web/foi_requests/models.py:97
msgid "Federal Public Bodies must not have a \"municipality\"."
msgstr "Órgãos federais não podem ter uma \"cidade\"."

#: web/foi_requests/models.py:108
msgid "Delayed"
msgstr "Atrasado"

#: web/foi_requests/models.py:109
